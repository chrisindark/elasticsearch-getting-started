GET 	/ 				controllers.IndexController.index()

GET		/api			controllers.IndexController.get(input : String)
POST	/api			controllers.IndexController.post(request : Request)
PUT		/api			controllers.IndexController.put(request : Request)
DELETE	/api			controllers.IndexController.delete()

GET     /api/users/:id  controllers.UsersController.get(id: Long)